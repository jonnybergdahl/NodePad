{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "File": {
      "Directory": "Logs",
      "FileName": "nodepad-",
      "MinimumLevel": "Information",
      "UseUtcTimestamp": true,
      "DailyRolling": true
    }
  },
  "AllowedHosts": "*",
  "PagesDirectory": "Pages",
  "BackupDirectory": "Backups"
}